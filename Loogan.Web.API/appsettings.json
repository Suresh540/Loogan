{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.AspNetCore": "Error"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": "Error",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Server=tcp:loogandev.database.windows.net,1433;Initial Catalog=loogandevdb;Persist Security Info=False;User ID=looganread;Password=loogan@123;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;",
          "tableName": "Logs",
          "autoCreateSqlTable": true,
          "schemaName": "EventLogging",
          "batchPostingLimit": 1000,
          "period": "0.00:00:30"
        },
        "restrictedToMinimumLevel": "Warning"
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "Properties": {
      "ApplicationName": "Loogan.Web.UI"
    }
  },
  "ConnectionStrings": {
    "looganConnectionString": "Server=tcp:loogandev.database.windows.net,1433;Initial Catalog=loogandevdb;Persist Security Info=False;User ID=looganread;Password=loogan@123;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
    //"looganConnectionString": "Server=SURESHKALAGA;Database=Loogan;uid=sa;pwd=loogan123;TrustServerCertificate=True"
  },
  "AllowedHosts": "*"
}
